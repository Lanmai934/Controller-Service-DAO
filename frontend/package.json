{
  "name": "frontend-auto-sdk",
  "version": "1.0.0",
  "description": "前端项目 - 自动SDK生成示例",
  "main": "src/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate-sdk": "swagger-typescript-api generate -p ../openapi.json -o src/api -n api.ts --modular --axios",
    "generate-sdk-from-server": "openapi-generator-cli generate -i http://localhost:3000/api-docs/swagger.json -g typescript-axios -o src/api --additional-properties=supportsES6=true,withInterfaces=true,modelPropertyNaming=camelCase",
    "mock": "prism mock ../openapi.json --host 0.0.0.0 --port 4010",
    "sync-api": "npm run fetch-openapi && npm run generate-sdk",
    "fetch-openapi": "curl -o ../openapi.json http://localhost:3000/api-docs/swagger.json",
    "watch-api": "nodemon --watch ../openapi.json --exec \"npm run generate-sdk\"",
    "start": "concurrently \"npm run dev\" \"npm run watch-api\"",
    "setup": "npm install && npm run generate-sdk"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.3.8",
    "axios": "^1.6.0",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@stoplight/prism-cli": "^5.4.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "swagger-typescript-api": "^13.2.15",
    "typescript": "^5.2.0",
    "vite": "^5.0.0"
  },
  "type": "module"
}