# 服务器配置
PORT=3000
NODE_ENV=development

# 前端URL配置（支持多个URL，用逗号分隔）
FRONTEND_URLS=http://localhost:5173,http://localhost:3000,https://yourdomain.com

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=controller_service_dao
DB_USER=root
DB_PASSWORD=your_password

# JWT配置
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=24h

# 日志配置
LOG_LEVEL=info

# API配置
API_PREFIX=/api
API_VERSION=v1